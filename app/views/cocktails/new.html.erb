<div class="row forms">
  <div class="col-xs-4 col-xs-offset-4">
    <h1>add a new kawfee cocktail!</h1>
      <%= simple_form_for @cocktail do |d| %>
        <%= d.input :name, label: "what is your kawfee cocktail's name?" %>
        <%= d.input :photo, label: "photo", as: :attachinary %>
        <%= d.button :submit %>
      <% end %>
  </div>
</div>
